	author = {Romano, Paul K. and Horelik, Nicholas E. and Herman, Bryan R. and Nelson, Adam G. and Forget, Benoit and Smith, Kord},
	month = aug,
	year = {2015},
	keywords = {Monte Carlo, OpenMC, Parallel, Neutron transport, HDF5, XML},
	pages = {90--97},
	file = {ScienceDirect Full Text PDF:/home/ooblack/Zotero/storage/IW54R5QX/Romano et al. - 2015 - OpenMC A state-of-the-art Monte Carlo code for re.pdf:application/pdf;ScienceDirect Snapshot:/home/ooblack/Zotero/storage/ALT6V9G9/S030645491400379X.html:text/html;ScienceDirect Full Text PDF:/home/ooblack/Zotero/storage/9S36DPTN/Romano et al. - 2015 - OpenMC A state-of-the-art Monte Carlo code for re.pdf:application/pdf;ScienceDirect Snapshot:/home/ooblack/Zotero/storage/NZ4279HS/S030645491400379X.html:text/html},
