	month = sep,
	year = {2016},
	doi = {10.2172/1364498},
	file = {Snapshot:/home/ooblack/Zotero/storage/EB3N8II8/1364498.html:text/html;Full Text PDF:/home/ooblack/Zotero/storage/NFSNM4TH/Szilard et al. - 2016 - Economic and Market Challenges Facing the U.S. Nuc.pdf:application/pdf},
